
{
    "id": "power_switch",   //属性的唯一标识
    "name": "电灯开关",      //名称
    "desc": "控制电灯开灭",   //属性的详细描述
    "required": true,       //表示此属性是否必需包含，是
    "mode": "rw",           //属性的模式，r代表读，w代表写
    "define": {             //属性的数值定义
      "type": "bool",       //数值的类型，布尔
      "mapping": {          //具体数值的含义
        "0": "关",           //0表示灯关闭
        "1": "开"            //1表示灯打开
      }
    }
  }